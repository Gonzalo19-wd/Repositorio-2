private int funcionHash(String clave, int tam) {
    int hash = 0;
    for (int i = 0; i < clave.length(); i++) {
        hash ^= clave.charAt(i); // mezcla con XOR
    }
    return Math.abs(hash) % tam;
}
// estos solo requiere del nombre y lo transmforma en algun numero basado en las letras y amenos aue el nombre contenga las mismas letras se repetira
